<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.katherine_qj.xiyou_library">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" ></action>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.MyLoginActivity"></activity>
        <activity android:name=".view.activity.AboutOursActivity"></activity>
        <activity android:name=".view.activity.GetDetailActivity"></activity>
        <activity android:name=".view.activity.SearchActivity"></activity>
        <activity android:name=".view.activity.BookDetailActivity"></activity>
        <activity android:name=".view.activity.MyRentActivity"></activity>
        <activity android:name=".view.activity.MyHistoryActivity"></activity>
        <activity android:name=".view.activity.MyFavoriteActivity"></activity>
    </application>

</manifest>